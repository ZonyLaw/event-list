<h1>Sharing your experience</h1>

<h2>Event</h2>
<ul>
  <%@events.each do |event|%>
    <li>
      <%=event.name%>: <%=event.details%> |
      <%=link_to "details", event_path(event)%> |
      <%=link_to "update", edit_event_path(event)%> |
    </li>
  <%end%>

</ul>

<div>
  <%=link_to "new", new_event_path%>
</div>
